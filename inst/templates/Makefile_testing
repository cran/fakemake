.POSIX:

R_engine=Rscript-devel

all.Rout: message1.Rout warning2.Rout
	$(R_engine) --vanilla -e 'fakemake::sink_all("all.Rout", message("This is file all."))'

error2.Rout: warning2.Rout
	$(R_engine) --vanilla -e 'fakemake::sink_all("error2.Rout", stop(sub("Warning", "Error", sub("a warning", "an error", readLines("warning2.Rout")))))'

warning2.Rout: message2.Rout
	$(R_engine) --vanilla -e 'fakemake::sink_all("warning2.Rout", code = warning(sub("message", "warning", readLines("message2.Rout"))))'

message2.Rout: 
	$(R_engine) --vanilla -e 'fakemake::sink_all("message2.Rout", message("This is a message."))'

message1.Rout:
	$(R_engine) --vanilla -e 'fakemake::sink_all("message1.Rout", message("This is a message."))'
